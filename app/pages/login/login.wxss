.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.head-container {
  width: 160rpx;
  height: 160rpx;
  /* background: pink; */
  margin: 168rpx;
}

.head-container > image {
  height: 100%;
  width: 100%;
  border-radius: 50%;
}

.input-container {
  /* background: pink; */
}

.tell-container {
  display: flex;
  align-items: center;
  border-bottom: 1rpx solid #D8D8D8;
}

.key-container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #D8D8D8;
  margin-top: 40rpx;
}

.icon-container {
  width: 50rpx;
  height: 80rpx;
  position: relative;
  /* background: pink; */
}

.phone-icon {
  width: 25rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%)
}

.key-icon {
  width: 30rpx;
  height: 35rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%)
}

input {
  padding-right: 10rpx;
}

.ph-style {
  font-size: 28rpx;
  color: #AAAAAA;
}

.btn-container {
  margin-top: 170rpx;
}

