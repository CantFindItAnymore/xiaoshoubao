.container {
  height: 100%;
  background: #FAFAFA;
  display: flex;
  flex-direction: column;
  align-items: center;
  padding-top: 60rpx;
  box-sizing: border-box;
}

.list-container {
  width: 660rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  background: #FAFAFA;
}

.info-container {
  margin-top: 30rpx;
}

.no-data-container {
  position: absolute;
  top: 26%;
  left: 50%;
  transform: translate(-50%, -50%);
  font-size: 32rpx;
  color: #AAAAAA;
}